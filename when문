when (x) {
  1 -> print("x==1")
  2 -> print("x==2")
  else -> {
  print(" x is neither 1 nor 2")
  }
}
---------------------------------
when (x){
  in 1..10 -> print("x는 1부터 10범위안에 있다")
  !in 10..20 -> print("x는 10부터 20범위 안에 없음")
  else -> print("otherwise")
}
------------------------------------
when (x) {
  is Int -> print("x는 인트형")
  else -> print("x는 인트형이 아니다")
}
